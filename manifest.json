{
  "manifest_version": 2,
  "name": "Copy Link For Markdown Format",
  "version": "1.0.0",
  "icons": {
    "16": "cow.png",
    "48": "cow.png",
    "128": "cow.png"
  },
  "permissions": [
    "tabs",
    "clipboardWrite"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon" : "cow.png",
    "default_title": "Copy Link For Markdown Format"
  }
}
